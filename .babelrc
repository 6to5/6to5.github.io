{
  "env": {
    "production": {
      "plugins":[
        "emotion"
      ]
    },
    "development": {
      "plugins": [
        ["emotion", { "sourceMap": true }]
      ]
    }
  },
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 3 versions", "ie >= 11"]
      }
    }],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}
