{
  "env": {
    "production": {
      "plugins":[
        "emotion"
      ]
    },
    "development": {
      "plugins": [
        ["emotion", { "sourceMap": true }]
      ]
    }
  },
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": ["last 3 versions"]
      }
    }],
    "@babel/flow",
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
